{
  "name": "fintorlyclient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-fast": "react-native run-android --active-arch-only ",
    "ios": "react-native run-ios --udid='7080AC0E-2D09-4EA7-9240-BA1FEB9F9354' ",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "cd ios && pod install",
    "postuninstall": "cd ios && pod install",
    "generate-mobile-api": "node fetchSwagger.js && npx @openapitools/openapi-generator-cli generate --skip-validate-spec -i ./swagger.json -g typescript-axios -o ./src/Mobile-Api/ -Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true && prettier --write ./src/Mobile-Api/api.ts",
    "check-dependencies": "rnx-dep-check",
    "fix-dependencies": "rnx-dep-check --write"
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@ui-kitten/components": "^5.1.2",
    "lottie-ios": "3.4.1",
    "lottie-react-native": "^5.1.4",
    "react": "18.1.0",
    "react-i18next": "^12.1.1",
    "react-native": "0.70.6",
    "react-native-config": "^1.4.11",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-reanimated": "^2.13.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
